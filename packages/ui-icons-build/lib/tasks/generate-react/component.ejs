import React, { Component } from 'react'
import SVGIcon from '@instructure/ui-svg-images/lib/components/SVGIcon'

export default class <%= name %> extends Component {
  static glyphName = '<%= glyphName %>';
  static variant = '<%= variant %>';
  <% if (deprecated) { %>
  static deprecated = true;
  <% } %>

  static propTypes = {
    ...SVGIcon.propTypes
  }

  render () {
    <% if (deprecated) { %>
    if (process.env.NODE_ENV !== 'production') {
      console.warn('<<%= name %> /> is deprecated. Please use <<%= deprecated %> /> instead.')
    }
    <% } %>
    return (
      <SVGIcon
        {...this.props}
        name="<%= name %>"
        viewBox="<%= viewBox %>"
      >
        <%= source %>
      </SVGIcon>
    )
  }
}
