{
  "stage": 0,
  "env": {
    "development": {
      "plugins": [
        "react-transform",
        "react-display-name"
      ],
      "extra": {
        "react-transform": {
          "transforms": [
            {
              "transform": "react-transform-hmr",
              "imports": ["react"],
              "locals": ["module"]
            }, {
              "transform": "react-transform-catch-errors",
              "imports": ["react", "redbox-react"]
            }
          ]
        }
      }
    },
    "production": {
      "plugins": [
        "react-display-name"
      ]
    }
  }
}
